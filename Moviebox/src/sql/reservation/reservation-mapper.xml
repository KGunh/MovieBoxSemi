<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<comment>
		Reservation Mapper
	</comment>
	
	<entry key="selectMovieList">
		SELECT
		       MOVIE_NO,
		       MOVIE_TITLE
		  FROM
		       TB_MOVIE
		 WHERE
		       STATUS = 'Y'
	</entry>
	
	
	
	
</properties>
